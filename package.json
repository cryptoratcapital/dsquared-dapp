{
  "name": "dsquared-dapp",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "typechain --target=ethers-v5 --out-dir common/abi/typechain common/abi/json/*.json",
    "precommit": "lint-staged",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check": "npm-run-all check:*",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --write .",
    "check:lint": "eslint .",
    "test:ci-only": "npm-run-all -p --race start test:headless",
    "test:monitoring-only": "node ./tests/utils/monitoring-runner.mjs",
    "test": "SERIAL_MODE=true env-cmd playwright test --project=chromium",
    "test:headless": "HEADLESS_MODE=true npm run test"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.43.15",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/providers": "^5.7.2",
    "@next/font": "13.2.1",
    "@playwright/test": "^1.32.1",
    "@sentry/nextjs": "^7.39.0",
    "@sentry/tracing": "^7.39.0",
    "@synthetixio/synpress": "^3.5.1",
    "@tanstack/react-query": "^4.22.4",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "ethers": "^5.1.3",
    "framer-motion": "^10.0.1",
    "husky": "^8.0.0",
    "keccak256": "^1.0.6",
    "lint-staged": "^13.1.0",
    "merkletreejs": "^0.3.9",
    "mock-socket": "^9.2.1",
    "next": "13.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "tailwindcss": "^3.2.4",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4",
    "vercel": "^28.15.0",
    "web3modal": "^1.9.12",
    "zustand": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run check:format",
      "npm run check:lint"
    ]
  }
}
